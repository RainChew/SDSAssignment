df <- read.csv("External_Trade_Monthly.csv")
print(df)
df$Gross_Exports <- as.numeric(gsub(",", "", df$Gross_Exports))
df$Gross_Imports <- as.numeric(gsub(",", "", df$Gross_Imports))
df$Total_Trade <- as.numeric(gsub(",", "", df$Total_Trade))
df$Trade_Balance <- as.numeric(gsub(",", "", df$Trade_Balance))
df
df <- ts(df, frequency = 12, start =c(2010,1))
# df <- log(df)
plot.ts(df)
# Convert "Period" to a Date object
data$Period <- as.Date(data$Period, format = "%m/%d/%Y")
# Create a univariate time series object for "Total_Trade"
total_trade_ts <- ts(data$Total_Trade, frequency = 12, start = c(2010, 1))
# Fit an ARIMA model with automatic order selection
auto_arima_model <- auto.arima(total_trade_ts)
# Print the selected model's summary
summary(auto_arima_model)
# Plot the ACF and PACF of the residuals
acf(residuals(auto_arima_model))
pacf(residuals(auto_arima_model))
